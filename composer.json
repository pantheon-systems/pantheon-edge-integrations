{
  "name": "pantheon-systems/pantheon-edge-integrations",
  "description": "Helper class for content personalization.",
  "license": "MIT",
  "type": "library",
  "autoload": {
    "classmap": [
      "src/"
    ],
    "psr-4": {
      "Pantheon\\EI\\": "src"
    }
  },
  "scripts": {
    "cbf": "vendor/bin/phpcbf --standard=PSR2 src",
    "lint": "vendor/bin/phpcs --standard=PSR2 src",
    "test": "vendor/bin/phpunit -c phpunit.xml"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "squizlabs/php_codesniffer": "^3.6"
  }
}
